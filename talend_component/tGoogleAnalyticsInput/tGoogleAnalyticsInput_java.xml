<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="5.1" STATUS="PROD"
		COMPATIBILITY="ALL" AUTHOR="Jan Lolling" RELEASE_DATE="20150204A"
		STARTABLE="true" HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Business/Google</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR 
			CTYPE="FLOW" 
			MAX_INPUT="0" 
			MAX_OUTPUT="1" 
			LINE_STYLE="1" 
			SHOW_IF="NORMALIZED_OUTPUT == 'false'" 
			/>
		<CONNECTOR 
			NAME="DIMENSION_VALUES" 
			CTYPE="FLOW" 
			MAX_INPUT="0"
			MAX_OUTPUT="1" 
			LINE_STYLE="1" 
			SHOW_IF="NORMALIZED_OUTPUT == 'true'"
			/>
		<CONNECTOR 
			NAME="METRIC_VALUES" 
			CTYPE="FLOW" 
			MAX_INPUT="0"
			MAX_OUTPUT="1" 
			LINE_STYLE="1" 
			SHOW_IF="NORMALIZED_OUTPUT == 'true'"
			/>
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER FIELD="TEXT" NAME="APPLICATION_NAME" NUM_ROW="10" GROUP="CLIENT">
			<DEFAULT>"Fetch Analytics"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			FIELD="CLOSED_LIST" 
			NAME="AUTHENTICATION_METHOD" 
			NUM_ROW="15" 
			REQUIRED="true" 
			GROUP="CLIENT">
	    	<ITEMS DEFAULT="SERVICE_ACCOUNT">
	    		<ITEM NAME="SERVICE_ACCOUNT" VALUE="SERVICE_ACCOUNT" />
	    		<ITEM NAME="APPLICATION_CLIENT_ID" VALUE="APPLICATION_CLIENT_ID" />
	    	</ITEMS>
		</PARAMETER>
		<PARAMETER 
			FIELD="TEXT" 
			NAME="SERVICE_ACCOUNT_EMAIL" 
			NUM_ROW="20" 
			REQUIRED="true" 
			GROUP="CLIENT" 
			SHOW_IF="AUTHENTICATION_METHOD == 'SERVICE_ACCOUNT'">
			<DEFAULT>"[service-account-id]developer.gserviceaccount.com"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			FIELD="FILE" 
			NAME="KEY_FILE" 
			NUM_ROW="21" 
			REQUIRED="true" 
			GROUP="CLIENT" 
			SHOW_IF="AUTHENTICATION_METHOD == 'SERVICE_ACCOUNT'">
			<DEFAULT>"/tmp/76548c6bc96518473d7e425e1415a14e495d34eb-privatekey.p12"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			FIELD="TEXT" 
			NAME="USER_ACCOUNT_EMAIL" 
			NUM_ROW="20" 
			REQUIRED="true" 
			GROUP="CLIENT" 
			SHOW_IF="AUTHENTICATION_METHOD == 'APPLICATION_CLIENT_ID'">
			<DEFAULT>"yourmail@gmail.com"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="FILE" NAME="CLIENT_SECRET_FILE" NUM_ROW="21" REQUIRED="true" GROUP="CLIENT" 
			SHOW_IF="AUTHENTICATION_METHOD == 'APPLICATION_CLIENT_ID'">
			<DEFAULT>"/tmp/client_secret.json"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="PROFILE_ID" NUM_ROW="35" REQUIRED="true" GROUP="QUERY">
			<DEFAULT>"59815695"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="START_DATE" NUM_ROW="40" REQUIRED="true" GROUP="QUERY"/>
		<PARAMETER FIELD="TEXT" NAME="END_DATE" NUM_ROW="50" REQUIRED="true" GROUP="QUERY"/>
		<PARAMETER FIELD="TEXT" NAME="DIMENSIONS" NUM_ROW="60" GROUP="QUERY">
			<DEFAULT>"ga:source,ga:keyword"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="METRICS" NUM_ROW="70"	REQUIRED="true" GROUP="QUERY">
			<DEFAULT>"ga:visits,ga:exits,ga:goal1Starts,ga:goal1Completions"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="FILTERS" NUM_ROW="75" GROUP="QUERY">
			<DEFAULT>"ga:medium==organic"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="SEGMENT" NUM_ROW="78" GROUP="QUERY">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="SORTS" NUM_ROW="80" GROUP="QUERY">
			<DEFAULT>"-ga:visits,ga:source"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SAMPLING_LEVEL" FIELD="CLOSED_LIST" NUM_ROW="85" GROUP="QUERY">
	    	<ITEMS DEFAULT="DEFAULT">
	    		<ITEM NAME="DEFAULT" VALUE="DEFAULT" />
	    		<ITEM NAME="FASTER" VALUE="FASTER" />
	    		<ITEM NAME="HIGHER_PRECISION" VALUE="HIGHER_PRECISION" />
	    	</ITEMS>
	    </PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="DELIVER_TOTALS_DATASET" REQUIRED="true" NUM_ROW="87" GROUP="QUERY">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="NORMALIZED_OUTPUT" NUM_ROW="88" GROUP="QUERY">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="DENORMALIZE_DATE" NUM_ROW="89" GROUP="QUERY" SHOW_IF="NORMALIZED_OUTPUT == 'true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="false" NUM_ROW="90" GROUP="QUERY" SHOW_IF="NORMALIZED_OUTPUT == 'false'">
			<DEFAULT />
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_DIMENSIONS" FIELD="SCHEMA_TYPE"
			REQUIRED="false" NUM_ROW="90" CONTEXT="DIMENSION_VALUES" GROUP="QUERY" SHOW_IF="NORMALIZED_OUTPUT == 'true'">
			<TABLE READONLY="true">
				<COLUMN NAME="ROW_NUM" TYPE="id_Integer" READONLY="true"
					CUSTOM="true" LENGTH="22" KEY="true" NULLABLE="false" COMMENT="Row number within the report result"/>
				<COLUMN NAME="DIMENSION_NAME" TYPE="id_String" READONLY="true"
					CUSTOM="true" LENGTH="64" NULLABLE="false" COMMENT="Name of the dimension"/>
				<COLUMN NAME="DIMENSION_VALUE" TYPE="id_String" READONLY="true"
					CUSTOM="true" LENGTH="128" NULLABLE="true" COMMENT="Value of the dimension"/>
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_METRICS" FIELD="SCHEMA_TYPE"
			REQUIRED="false" NUM_ROW="91" CONTEXT="METRIC_VALUES" GROUP="QUERY" SHOW_IF="NORMALIZED_OUTPUT == 'true'">
			<TABLE READONLY="true">
				<COLUMN NAME="ROW_NUM" TYPE="id_Integer" READONLY="true"
					CUSTOM="true" LENGTH="22" KEY="true" NULLABLE="false" COMMENT="Row number within the report result"/>
				<COLUMN NAME="METRIC_NAME" TYPE="id_String" READONLY="true"
					CUSTOM="true" LENGTH="64" NULLABLE="false" COMMENT="Name of the metric"/>
				<COLUMN NAME="METRIC_VALUE" TYPE="id_Double" READONLY="true"
					CUSTOM="true" LENGTH="22" PRECISION="4" NULLABLE="true" COMMENT="Value of the metric"/>
			</TABLE>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="DIE_ON_ERROR" NUM_ROW="100" >
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
		<PARAMETER FIELD="TEXT" NAME="TIMEOUT_IN_SEC" NUM_ROW="10" >
			<DEFAULT>240</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="TIME_OFFSET" NUM_ROW="15" >
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="FETCH_SIZE" NUM_ROW="20" >
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="NUMBERFORMAT_LOCALE" NUM_ROW="30" REQUIRED="true">
			<DEFAULT>"en"</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="CHECK" NAME="KEEP_CLIENT" NUM_ROW="40" >
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		<PARAMETER FIELD="TEXT" NAME="KEEP_CLIENT_KEY" NUM_ROW="40" 
		           SHOW_IF="KEEP_CLIENT == 'true'">
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Google Analytics"
				MODULE="jlo.talendcomp.gaanalytics-5.1-jar-with-dependencies.jar"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE_DIMENSIONS" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE_METRICS" TYPE="id_Integer" />
		<RETURN AVAILABILITY="AFTER" NAME="CONTAINS_SAMPLED_DATA" TYPE="id_Boolean" />
		<RETURN AVAILABILITY="AFTER" NAME="SAMPLE_SIZE" TYPE="id_Long" />
		<RETURN AVAILABILITY="AFTER" NAME="SAMPLE_SPACE" TYPE="id_Long" />
		<RETURN AVAILABILITY="AFTER" NAME="TOTAL_AFFECTED_ROWS" TYPE="id_Integer" />
		<RETURN AVAILABILITY="FLOW" NAME="CURRENT_DATE" TYPE="id_Date" />
		<RETURN AVAILABILITY="AFTER" NAME="ERROR_CODE" TYPE="id_Integer" />
	</RETURNS>
</COMPONENT>
